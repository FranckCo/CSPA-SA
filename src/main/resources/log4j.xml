<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE log4j:configuration SYSTEM "log4j.dtd"><log4j:configuration xmlns:log4j="http://jakarta.apache.org/log4j/" debug="false">	<appender name="file" class="org.apache.log4j.FileAppender">		<param name="File" value="sa-service.log"/>		<layout class="org.apache.log4j.PatternLayout">			<param name="ConversionPattern" value="%-5p %d{dd-MM-yy HH:mm:ss,SSS} %m [%F:%L]%n"/>		</layout>	</appender>	<appender name="console" class="org.apache.log4j.ConsoleAppender">		<layout class="org.apache.log4j.PatternLayout">			<param name="ConversionPattern" value="%-5p %d{dd-MM-yy HH:mm:ss,SSS} %m [%F:%L]%n"/>		</layout>	</appender>	<logger name="fr.insee.cspa.sa" additivity="false">		<level value="debug"/>		<appender-ref ref="file"/>	</logger>	<root>		<level value="info"/>		<appender-ref ref="console"/>	</root></log4j:configuration>